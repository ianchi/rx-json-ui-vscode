{
    "name": "rx-json-ui-vscode",
    "displayName": "rx-json-ui-vscode",
    "description": "Adds expression validation to json schema",
    "publisher": "ianchi",
    "author": {
        "name": "Adrian Panella",
        "email": "ianchi74@outlook.com"
    },
    "version": "0.0.2",
    "license": "MIT",
    "engines": {
        "vscode": "^1.41.0"
    },
    "activationEvents": [
        "onLanguage:json",
        "onLanguage:jsonc"
    ],
    "main": "./client/dist/clientMain",
    "enableProposedApi": true,
    "scripts": {
        "compile": "tsc -p client && tsc -p server",
        "build": "webpack --config extension.webpack.config.js && webpack --config server/extension.webpack.config.js",
        "postinstall": "cd server && npm install",
        "package": "vsce package"
    },
    "categories": [
        "Programming Languages"
    ],
    "dependencies": {
        "request-light": "^0.2.5",
        "vscode-languageclient": "^6.0.0-next.3"
    },
    "devDependencies": {
        "@types/node": "^12.11.7",
        "@types/vscode": "^1.41.0",
        "copy-webpack-plugin": "^5.1.1",
        "merge-options": "^2.0.0",
        "rx-json-ui-cli": "0.0.2",
        "ts-loader": "^6.2.1",
        "typescript": "^3.7.3",
        "webpack": "^4.41.4",
        "webpack-cli": "^3.3.10"
    }
}
